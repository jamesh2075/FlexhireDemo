{
  "name": "flexhiredemo.client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-script-os",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prestart": "node aspnetcore-https",
    "start:windows": "ng serve --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\" --host=127.0.0.1",
    "start:default": "ng serve --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\" --host=127.0.0.1"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^17",
    "@angular/animations": "^17",
    "@angular/common": "^17",
    "@angular/compiler": "^17",
    "@angular/core": "^17",
    "@angular/forms": "^17",
    "@angular/platform-browser": "^17",
    "@angular/platform-browser-dynamic": "^17",
    "@angular/router": "^17",
    "@microsoft/signalr": "^9.0.6",
    "rxjs": "^7.8.2",
    "zone.js": "^0.14.3"
  },
  "devDependencies": {
    "@angular/cli": "^20.2.2",
    "@angular/compiler-cli": "^17",
    "run-script-os": "^1.1.6",
    "typescript": "~5.2.0"
  }
}
